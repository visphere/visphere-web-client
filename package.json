{
  "name": "moonsphere-web-client",
  "version": "1.0.0",
  "scripts": {
    "start:dev": "npx webpack-dev-server --progress --config _webpack/webpack-dev.config.cjs",
    "docker:dev": "npx webpack --progress --config _webpack/webpack-docker-dev.config.cjs",
    "docker:prod": "npx webpack --progress --config _webpack/webpack-docker-prod.config.cjs",
    "lint:check": "npx eslint src/**/*.{ts,html}",
    "lint:fix": "npx eslint src/**/*.{ts,html} --fix",
    "format": "npx prettier . --write",
    "test:single": "cross-env ENV_KARMA_TYPE='single' npx karma start karma.conf.cjs",
    "test:watch": "cross-env ENV_KARMA_TYPE='watch' npx karma start karma.conf.cjs",
    "husky:add-pre-commit": "npx husky add .husky/pre-commit \"npx --no-install lint-staged\"",
    "husky:add-pre-push": "npx husky add .husky/pre-push \"yarn run test:single\"",
    "configure:husky": "npx husky install && yarn run husky:add-pre-commit && yarn run husky:add-pre-push"
  },
  "private": true,
  "author": {
    "name": "MoonSphere Systems",
    "email": "info@moonsphere.pl",
    "url": "https://moonsphere.pl"
  },
  "license": "Apache-2.0",
  "lint-staged": {
    "**/*.{ts,html,ejs,js,cjs,json}": [
      "yarn run lint:fix",
      "yarn run format"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run test:single"
    }
  },
  "dependencies": {
    "@angular/animations": "^16.1.0",
    "@angular/common": "^16.1.0",
    "@angular/compiler": "^16.1.0",
    "@angular/core": "^16.1.0",
    "@angular/forms": "^16.1.0",
    "@angular/platform-browser": "^16.1.0",
    "@angular/platform-browser-dynamic": "^16.1.0",
    "@angular/router": "^16.1.0",
    "@messageformat/core": "^3.2.0",
    "@ng-icons/bootstrap-icons": "^25.1.0",
    "@ng-icons/core": "^25.1.0",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
    "@zxcvbn-ts/core": "^3.0.2",
    "dompurify": "^3.0.5",
    "moment": "^2.29.4",
    "ngx-translate-messageformat-compiler": "^6.5.0",
    "rxjs": "^7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "^0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.1.5",
    "@angular-eslint/builder": "^16.1.0",
    "@angular-eslint/eslint-plugin": "^16.1.0",
    "@angular-eslint/eslint-plugin-template": "^16.1.0",
    "@angular-eslint/schematics": "^16.1.0",
    "@angular-eslint/template-parser": "^16.1.0",
    "@angular/cli": "^16.1.5",
    "@angular/compiler-cli": "^16.1.5",
    "@babel/core": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "@ngtools/webpack": "^16.1.5",
    "@tailwindcss/forms": "^0.5.4",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/dompurify": "^3.0.2",
    "@types/jasmine": "^4.3.5",
    "@types/node": "^20.4.4",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vicgutt/tailwindcss-font-face": "^0.2.0",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^9.1.3",
    "clean-webpack-plugin": "^4.0.0",
    "compression-webpack-plugin": "^10.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "dotenv": "^16.3.1",
    "eslint": "^8.44.0",
    "eslint-plugin-json": "^3.1.0",
    "html-webpack-plugin": "^5.5.3",
    "husky": "^8.0.3",
    "karma": "6.4.2",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-remap-coverage": "^0.1.5",
    "karma-source-map-support": "^1.4.0",
