    "tailwind-scrollbar-variants": "^1.1.1",
    "tailwindcss": "^3.3.3",
    "terser-webpack-plugin": "^5.3.9",
    "typescript": "^5.1.6",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.9.0",
    "webpack-remove-empty-scripts": "^1.0.3"
  }
}
